#include <stdio.h>
#define MAX 100
int list[MAX];
int n = 0;
void insertInBeg(int value){
       if(n==MAX){
        printf("List is full !!!!\n");
        return;
    }
    for(int i = n - 1; i >= 0; i--){
        list[i+1]= list[i];
    }
    list[0] = value;
    n++;
    printf("Element Inserted!!\n");
}
void displayList(){
    if(n == 0){
        printf("List IS Empty");
        return;
    }
    for (int i = 0; i < n; i++){
        printf("%d\t", list[i]);
    }
    printf("\n");
}
int main(){
    
    while(1){
        printf("1. Insert In Beginning\n");
        printf("2. Insert At End\n");
        printf("3.Insert At Any Position\n");
        printf("4.Delete From Beginning\n");
        printf("5.Delete From End\n");
        printf("6.Delete Form Given Position\n");
        printf("7.Display List\n");
        printf("8.Search Element In list\n");
        printf("9.Update Elements of List\n");
        printf("10. Sort Element of list\n ");
        printf("11. Exit\n");
        int choice, data;
        printf("Enter Choice: ");
        scanf("%d", &choice);
        switch(choice){
            case 1: 
                 printf("Enter Data :");
                 scanf("%d", &data);
                 insertInBeg(data);
                 break;
            case 2:
                 break;
            case 3:
                 break;
            case 4:
                 break;
            case 5:
                 break;
            case 6:
                 break;
            case 7: 
                 displayList();
                 break;
            case 8: 
                 break;
            case 9: 
                 break;
            case 10: 
                 break;
            case 11: 
                 break;
            default:
                printf("Invalid choice!!!!\n ");
        }
        printf("\n");
    }

    return 0;
}

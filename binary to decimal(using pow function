#include<stdio.h>
#include<math.h>
int main(){
    int num, p = 0;
    int rem = 0, sum = 0;
    printf("Enter the binary number: ");
    scanf("%d", &num);
    int temp = num;
    while( num != 0){
        rem = num%10;
        sum = sum + rem*pow(2,p);
        p++;
        num = num/10;
    }
    printf("Decimal of %d is %d", temp, sum);
    return 0;
}
